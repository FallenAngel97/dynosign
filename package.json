{
  "name": "dynosign",
  "version": "1.0.1",
  "author": {
    "name": "F97A"
  },
  "description": "Crossplatform UI/UX tool",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "watch": "webpack-dev-server --hot --inline --config webpack.dev.js",
    "test": "cross-env ELECTRON_RUN_AS_NODE=1 electron ./node_modules/.bin/jest",
    "test:watch": "cross-env ELECTRON_RUN_AS_NODE=1 ./node_modules/.bin/electron ./node_modules/.bin/jest -- --watch .",
    "build": "webpack --config webpack.prod.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "doc": "documentation build ProgramEntry/** SettingsPage/** -f html -o gh-pages --github --theme gh-pages/documentationjs_theme",
    "get-coverage": "cross-env ELECTRON_RUN_AS_NODE=1 ./node_modules/.bin/electron ./node_modules/.bin/jest  --coverage"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "electron": "<rootDir>/src/components/tests/mock/electron.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FallenAngel97/dynosign.git"
  },
  "bugs": {
    "url": "https://github.com/FallenAngel97/dynosign/issues"
  },
  "homepage": "https://github.com/FallenAngel97/dynosign#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.4",
    "closure-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "1.0.1",
    "documentation": "^9.3.1",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-packager": "13.0.0",
    "electron-rebuild": "1.8.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^2.0.0",
    "google-closure-compiler": "^20190301.0.0",
    "jest": "^24.7.1",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-libs-browser": "2.1.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prepack-webpack-plugin": "^1.1.2",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^6.0.1",
    "sass": "1.15.2",
    "sass-loader": "7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "4.27.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "electron-store": "^3.2.0",
    "font-manager": "^0.3.0",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-color": "^2.17.1",
    "react-dom": "^16.8.6",
    "react-localize-redux": "^3.5.3",
    "react-select": "^2.4.3",
    "redux": "^4.0.1"
  },
  "build": {
    "appId": "dynosign",
    "publish": {
      "provider": "github",
      "releaseType": "release"
    },
    "mac": {
      "category": "public.app-category.photography"
    },
    "linux": {
      "category": "Graphics"
    },
    "files": [
      "!**/node_modules/*",
      "index.html",
      "main.js",
      "package.json",
      "package-lock.json",
      "dist/*.svg",
      "dist/*.png",
      "dist/*.js",
      "dist/*.otf",
      "dist/*.css"
    ]
  }
}
